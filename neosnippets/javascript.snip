snippet cl
options head
  console.log(${0});

snippet f
options word
  function ${1} (${2}) {
    ${0}
  }

snippet if
options head
  if (${1}) {
    ${0}
  }

snippet ife
options head
  if (${1:#:condition}) {
    ${2}
  } else {
    ${0}
  }

snippet for
options head
  for (var ${1:i} = 0; $1 < ${2:#:Things}.length; ++$1) {
    ${0}
  }

snippet forin
options head
  for (var ${1:i} in ${2:#:Things}) {
    ${0:TARGET}
  }
